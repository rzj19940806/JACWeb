@{
    ViewBag.Title = "Base_Product》明细页面";
    Layout = "~/Views/Shared/_LayoutForm.cshtml";
}
<script type="text/javascript">
    $(function () {
        InitControl();
    })
    //得到一个对象实体
    function InitControl() {
        if (!!GetQuery('KeyValue')) {
            AjaxJson("/CommonModule/Product/SetForm", { KeyValue: GetQuery('KeyValue') }, function (data) {
                SetWebControls(data);
            });
        }
    }
</script>
<form id="form1" style="margin: 1px">
    <table class="form">
        <tr>
            <th class='formTitle'>ProductId：</th>
            <td class='formValue'>
                <label id="ProductId" />
            </td>
            <th class='formTitle'>ProductName：</th>
            <td class='formValue'>
                <label id="ProductName" />
            </td>
        </tr>
        <tr>
        </tr>
    </table>
</form>

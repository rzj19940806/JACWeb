@{
    ViewBag.Title = "Base_Factory》明细页面";
    Layout = "~/Views/Shared/_LayoutForm.cshtml";
}
<script type="text/javascript">
    $(function () {
        InitControl();
    })
    //得到一个对象实体
    function InitControl() {
        if (!!GetQuery('KeyValue')) {
            AjaxJson("/CommonModule/Factory/SetForm", { KeyValue: GetQuery('KeyValue') }, function (data) {
                SetWebControls(data);
            });
        }
    }
</script>
<form id="form1" style="margin: 1px">
    <table class="form">
        <tr>
            <th class='formTitle'>FactoryId：</th>
            <td class='formValue'>
                <label id="FactoryId" />
            </td>
            <th class='formTitle'>FactoryCode：</th>
            <td class='formValue'>
                <label id="FactoryCode" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>FactoryName：</th>
            <td class='formValue'>
                <label id="FactoryName" />
            </td>
            <th class='formTitle'>Description：</th>
            <td class='formValue'>
                <label id="Description" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>ParentId：</th>
            <td class='formValue'>
                <label id="ParentId" />
            </td>
        </tr>
    </table>
</form>

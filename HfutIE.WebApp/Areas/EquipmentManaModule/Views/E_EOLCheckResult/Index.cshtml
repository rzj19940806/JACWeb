    //上标签切换
    function changeupTab(id) {
        $(".upDiv").hide();
        $("#" + id + "Page").show();
        $(".upTab").removeClass("actived");
        $("#Tabcheck" + id).addClass("actived");
        if (id =="TESTLine") {
            TESTLineTabchange("Speed");
        } else if (id == "JZ") {
            JZTabchange("EHYFDY");
        }
    }

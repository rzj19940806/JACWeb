@{
    ViewBag.Title = "系统异常日志表》明细页面";
    Layout = "~/Views/Shared/_LayoutForm.cshtml";
}
<script type="text/javascript">
    $(function () {
        InitControl();
    })
    //得到一个对象实体
    function InitControl() {
        if (!!GetQuery('KeyValue')) {
            AjaxJson("/CommonModule/S_LogErr/SetForm", { KeyValue: GetQuery('KeyValue') }, function (data) {
                SetWebControls(data);
            });
        }
    }
</script>
<form id="form1" style="margin: 1px">
    <table class="form">
        <tr>
            <th class='formTitle'>主键：</th>
            <td class='formValue'>
                <label id="LogErrId" />
            </td>
            <th class='formTitle'>记录时间：</th>
            <td class='formValue'>
                <label id="RecTm" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>方法名=>异常堆栈：</th>
            <td class='formValue'>
                <label id="MethNm" />
            </td>
            <th class='formTitle'>方法参数：</th>
            <td class='formValue'>
                <label id="ErrFrom" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>错误详情：</th>
            <td class='formValue'>
                <label id="ErrDetl" />
            </td>
            <th class='formTitle'>备注：</th>
            <td class='formValue'>
                <label id="Rem" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>预留字段：</th>
            <td class='formValue'>
                <label id="RsvFld1" />
            </td>
        </tr>
    </table>
</form>

@{
    ViewBag.Title = "工位基础信息表》明细页面";
    Layout = "~/Views/Shared/_LayoutForm.cshtml";
}
<script type="text/javascript">
    $(function () {
        InitControl();
    })
    //得到一个对象实体
    function InitControl() {
        if (!!GetQuery('KeyValue')) {
            AjaxJson("/CommonModule/BBdbR_WcBase/SetForm", { KeyValue: GetQuery('KeyValue') }, function (data) {
                SetWebControls(data);
            });
        }
    }
</script>
<form id="form1" style="margin: 1px">
    <table class="form">
        <tr>
            <th class='formTitle'>工位主键：</th>
            <td class='formValue'>
                <label id="WcId" />
            </td>
            <th class='formTitle'>产线主键：</th>
            <td class='formValue'>
                <label id="PlineId" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>工位编号：</th>
            <td class='formValue'>
                <label id="WcCd" />
            </td>
            <th class='formTitle'>工位名称：</th>
            <td class='formValue'>
                <label id="WcNm" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>工位类型：</th>
            <td class='formValue'>
                <label id="WcTyp" />
            </td>
            <th class='formTitle'>工位顺序：</th>
            <td class='formValue'>
                <label id="WcQueue" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>工位长度：</th>
            <td class='formValue'>
                <label id="WcLength" />
            </td>
            <th class='formTitle'>工位开始：</th>
            <td class='formValue'>
                <label id="StartPoint" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>预警：</th>
            <td class='formValue'>
                <label id="PreAlarmPoint" />
            </td>
            <th class='formTitle'>结束：</th>
            <td class='formValue'>
                <label id="EndPoint" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>停止：</th>
            <td class='formValue'>
                <label id="StopPoint" />
            </td>
            <th class='formTitle'>排序号：</th>
            <td class='formValue'>
                <label id="Seq" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>工位描述：</th>
            <td class='formValue'>
                <label id="Dsc" />
            </td>
            <th class='formTitle'>版本号：</th>
            <td class='formValue'>
                <label id="VersionNumber" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>有效性：</th>
            <td class='formValue'>
                <label id="Enabled" />
            </td>
            <th class='formTitle'>创建时间：</th>
            <td class='formValue'>
                <label id="CreTm" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>创建人编号：</th>
            <td class='formValue'>
                <label id="CreCd" />
            </td>
            <th class='formTitle'>创建人名称：</th>
            <td class='formValue'>
                <label id="CreNm" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>修改时间：</th>
            <td class='formValue'>
                <label id="MdfTm" />
            </td>
            <th class='formTitle'>修改人编号：</th>
            <td class='formValue'>
                <label id="MdfCd" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>修改人名称：</th>
            <td class='formValue'>
                <label id="MdfNm" />
            </td>
            <th class='formTitle'>备注：</th>
            <td class='formValue'>
                <label id="Rem" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>预留字段1：</th>
            <td class='formValue'>
                <label id="RsvFld1" />
            </td>
            <th class='formTitle'>预留字段2：</th>
            <td class='formValue'>
                <label id="RsvFld2" />
            </td>
        </tr>
        <tr>
        </tr>
    </table>
</form>

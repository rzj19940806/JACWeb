@{
    ViewBag.Title = "质控点基本表》明细页面";
    Layout = "~/Views/Shared/_LayoutForm.cshtml";
}
<script type="text/javascript">
    $(function () {
        InitControl();
    })
    //得到一个对象实体
    function InitControl() {
        if (!!GetQuery('KeyValue')) {
            AjaxJson("/CommonModule/QualityCheckPointBase/SetForm", { KeyValue: GetQuery('KeyValue') }, function (data) {
                SetWebControls(data);
            });
        }
    }
</script>
<form id="form1" style="margin: 1px">
    <table class="form">
        <tr>
            <th class='formTitle'>质控点主键：</th>
            <td class='formValue'>
                <label id="QualityCheckPointId" />
            </td>
            <th class='formTitle'>质控点编号：</th>
            <td class='formValue'>
                <label id="QualityCheckPointCd" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>质控点名称：</th>
            <td class='formValue'>
                <label id="QualityCheckPointNm" />
            </td>
            <th class='formTitle'>质控点描述：</th>
            <td class='formValue'>
                <label id="QualityCheckPointDsc" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>顺序号：</th>
            <td class='formValue'>
                <label id="QueueNo" />
            </td>
            <th class='formTitle'>有效性：</th>
            <td class='formValue'>
                <label id="Enabled" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>版本号：</th>
            <td class='formValue'>
                <label id="VersionNumber" />
            </td>
            <th class='formTitle'>创建时间：</th>
            <td class='formValue'>
                <label id="CreTm" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>创建人编号：</th>
            <td class='formValue'>
                <label id="CreCd" />
            </td>
            <th class='formTitle'>创建人名称：</th>
            <td class='formValue'>
                <label id="CreNm" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>修改时间：</th>
            <td class='formValue'>
                <label id="MdfTm" />
            </td>
            <th class='formTitle'>修改人编号：</th>
            <td class='formValue'>
                <label id="MdfCd" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>修改人名称：</th>
            <td class='formValue'>
                <label id="MdfNm" />
            </td>
            <th class='formTitle'>备注：</th>
            <td class='formValue'>
                <label id="Rem" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>预留字段1：</th>
            <td class='formValue'>
                <label id="RsvFld1" />
            </td>
            <th class='formTitle'>预留字段2：</th>
            <td class='formValue'>
                <label id="RsvFld2" />
            </td>
        </tr>
        <tr>
        </tr>
    </table>
</form>

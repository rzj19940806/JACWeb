@{
    ViewBag.Title = "班次详细时间配置信息表》明细页面";
    Layout = "~/Views/Shared/_LayoutForm.cshtml";
}
<script type="text/javascript">
    $(function () {
        InitControl();
    })
    //得到一个对象实体
    function InitControl() {
        if (!!GetQuery('KeyValue')) {
            AjaxJson("/CommonModule/BFacR_ShiftTmConfig/SetForm", { KeyValue: GetQuery('KeyValue') }, function (data) {
                SetWebControls(data);
            });
        }
    }
</script>
<form id="form1" style="margin: 1px">
    <table class="form">
        <tr>
            <th class='formTitle'>主键：</th>
            <td class='formValue'>
                <label id="ConfigId" />
            </td>
            <th class='formTitle'>班次编号：</th>
            <td class='formValue'>
                <label id="ShiftCd" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>班次名称：</th>
            <td class='formValue'>
                <label id="ShiftNm" />
            </td>
            <th class='formTitle'>时间类型：</th>
            <td class='formValue'>
                <label id="RestTm" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>开始时间：</th>
            <td class='formValue'>
                <label id="StrtRestTm" />
            </td>
            <th class='formTitle'>结束时间：</th>
            <td class='formValue'>
                <label id="EndRestTm" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>版本号：</th>
            <td class='formValue'>
                <label id="VersionNumber" />
            </td>
            <th class='formTitle'>有效性：</th>
            <td class='formValue'>
                <label id="Enabled" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>创建时间：</th>
            <td class='formValue'>
                <label id="CreTm" />
            </td>
            <th class='formTitle'>创建人编号：</th>
            <td class='formValue'>
                <label id="CreCd" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>创建人名称：</th>
            <td class='formValue'>
                <label id="CreNm" />
            </td>
            <th class='formTitle'>修改时间：</th>
            <td class='formValue'>
                <label id="MdfTm" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>修改人编号：</th>
            <td class='formValue'>
                <label id="MdfCd" />
            </td>
            <th class='formTitle'>修改人名称：</th>
            <td class='formValue'>
                <label id="MdfNm" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>备注：</th>
            <td class='formValue'>
                <label id="Rem" />
            </td>
            <th class='formTitle'>预留字段1：</th>
            <td class='formValue'>
                <label id="RsvFld1" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>预留字段2：</th>
            <td class='formValue'>
                <label id="RsvFld2" />
            </td>
        </tr>
    </table>
</form>

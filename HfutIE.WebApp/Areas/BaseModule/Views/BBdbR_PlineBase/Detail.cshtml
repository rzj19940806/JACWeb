@{
    ViewBag.Title = "产线基础信息表》明细页面";
    Layout = "~/Views/Shared/_LayoutForm.cshtml";
}
<script type="text/javascript">
    $(function () {
        InitControl();
    })
    //得到一个对象实体
    function InitControl() {
        if (!!GetQuery('KeyValue')) {
            AjaxJson("/CommonModule/BBdbR_PlineBase/SetForm", { KeyValue: GetQuery('KeyValue') }, function (data) {
                SetWebControls(data);
            });
        }
    }
</script>
<form id="form1" style="margin: 1px">
    <table class="form">
        <tr>
            <th class='formTitle'>车间主键：</th>
            <td class='formValue'>
                <label id="WorkshopId" />
            </td>
            <th class='formTitle'>产线编号：</th>
            <td class='formValue'>
                <label id="PlineCd" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>产线名称：</th>
            <td class='formValue'>
                <label id="PlineNm" />
            </td>
            <th class='formTitle'>产线类型：</th>
            <td class='formValue'>
                <label id="PlineTyp" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>工位数量：</th>
            <td class='formValue'>
                <label id="WcQuantity" />
            </td>
            <th class='formTitle'>工位长度：</th>
            <td class='formValue'>
                <label id="WcLength" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>负责人：</th>
            <td class='formValue'>
                <label id="Mgr" />
            </td>
            <th class='formTitle'>产线描述：</th>
            <td class='formValue'>
                <label id="Dsc" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>有效性：</th>
            <td class='formValue'>
                <label id="Enabled" />
            </td>
            <th class='formTitle'>备注：</th>
            <td class='formValue'>
                <label id="Rem" />
            </td>
        </tr>
        <tr>
        </tr>
    </table>
</form>

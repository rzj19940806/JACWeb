@{
    ViewBag.Title = "生产信息类采集记录表》明细页面";
    Layout = "~/Views/Shared/_LayoutForm.cshtml";
}
<script type="text/javascript">
    $(function () {
        InitControl();
    })
    //得到一个对象实体
    function InitControl() {
        if (!!GetQuery('KeyValue')) {
            AjaxJson("/CommonModule/E_ProduceInfoRecord/SetForm", { KeyValue: GetQuery('KeyValue') }, function (data) {
                SetWebControls(data);
            });
        }
    }
</script>
<form id="form1" style="margin: 1px">
    <table class="form">
        <tr>
            <th class='formTitle'>主键：</th>
            <td class='formValue'>
                <label id="RecId" />
            </td>
            <th class='formTitle'>机构级别：</th>
            <td class='formValue'>
                <label id="Class" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>机构ID：</th>
            <td class='formValue'>
                <label id="ClassId" />
            </td>
            <th class='formTitle'>设备主键：</th>
            <td class='formValue'>
                <label id="DvcId" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>设备编号：</th>
            <td class='formValue'>
                <label id="DvcCd" />
            </td>
            <th class='formTitle'>设备名称：</th>
            <td class='formValue'>
                <label id="DvcNm" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>车身号：</th>
            <td class='formValue'>
                <label id="BodyNo" />
            </td>
            <th class='formTitle'>车型：</th>
            <td class='formValue'>
                <label id="CarType" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>车身物料号：</th>
            <td class='formValue'>
                <label id="MatCd" />
            </td>
            <th class='formTitle'>车身面漆颜色：</th>
            <td class='formValue'>
                <label id="CarColor1" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>车身罩光颜色：</th>
            <td class='formValue'>
                <label id="CarColor2" />
            </td>
            <th class='formTitle'>VIN码：</th>
            <td class='formValue'>
                <label id="VIN" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>流水号：</th>
            <td class='formValue'>
                <label id="SequenceNo" />
            </td>
            <th class='formTitle'>生产计数：</th>
            <td class='formValue'>
                <label id="Count" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>下线时间：</th>
            <td class='formValue'>
                <label id="OfflineTime" />
            </td>
            <th class='formTitle'>采集状态：</th>
            <td class='formValue'>
                <label id="CollectionStatus" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>采集日期：</th>
            <td class='formValue'>
                <label id="CollectionDate" />
            </td>
            <th class='formTitle'>采集时间：</th>
            <td class='formValue'>
                <label id="CollectionTime" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>创建时间：</th>
            <td class='formValue'>
                <label id="CreTm" />
            </td>
            <th class='formTitle'>备注：</th>
            <td class='formValue'>
                <label id="Rem" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>预留字段1：</th>
            <td class='formValue'>
                <label id="RsvFld1" />
            </td>
            <th class='formTitle'>预留字段2：</th>
            <td class='formValue'>
                <label id="RsvFld2" />
            </td>
        </tr>
        <tr>
        </tr>
    </table>
</form>

@{
    ViewBag.Title = "班制基础信息表》明细页面";
    Layout = "~/Views/Shared/_LayoutForm.cshtml";
}
<script type="text/javascript">
    $(function () {
        InitControl();
    })
    //得到一个对象实体
    function InitControl() {
        if (!!GetQuery('KeyValue')) {
            AjaxJson("/CommonModule/BFacR_ClassBase/SetForm", { KeyValue: GetQuery('KeyValue') }, function (data) {
                SetWebControls(data);
            });
        }
    }
</script>
<form id="form1" style="margin: 1px">
    <table class="form">
        <tr>
            <th class='formTitle'>班制主键：</th>
            <td class='formValue'>
                <label id="ClassId" />
            </td>
            <th class='formTitle'>班制编号：</th>
            <td class='formValue'>
                <label id="ClassCd" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>班制名称：</th>
            <td class='formValue'>
                <label id="ClassNm" />
            </td>
            <th class='formTitle'>班制类型：</th>
            <td class='formValue'>
                <label id="ClassTyp" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>班制开始时间：</th>
            <td class='formValue'>
                <label id="StartTm" />
            </td>
            <th class='formTitle'>班制总时长：</th>
            <td class='formValue'>
                <label id="TmSpan" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>版本号：</th>
            <td class='formValue'>
                <label id="VersionNumber" />
            </td>
            <th class='formTitle'>有效性：</th>
            <td class='formValue'>
                <label id="Enabled" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>创建时间：</th>
            <td class='formValue'>
                <label id="CreTm" />
            </td>
            <th class='formTitle'>创建人编号：</th>
            <td class='formValue'>
                <label id="CreCd" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>创建人名称：</th>
            <td class='formValue'>
                <label id="CreNm" />
            </td>
            <th class='formTitle'>修改时间：</th>
            <td class='formValue'>
                <label id="MdfTm" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>修改人编号：</th>
            <td class='formValue'>
                <label id="MdfCd" />
            </td>
            <th class='formTitle'>修改人名称：</th>
            <td class='formValue'>
                <label id="MdfNm" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>备注：</th>
            <td class='formValue'>
                <label id="Rem" />
            </td>
            <th class='formTitle'>预留字段1：</th>
            <td class='formValue'>
                <label id="RsvFld1" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>预留字段2：</th>
            <td class='formValue'>
                <label id="RsvFld2" />
            </td>
        </tr>
    </table>
</form>

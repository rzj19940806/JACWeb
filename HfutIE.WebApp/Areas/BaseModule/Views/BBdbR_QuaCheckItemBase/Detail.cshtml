@{
    ViewBag.Title = "检查项目基础信息表》明细页面";
    Layout = "~/Views/Shared/_LayoutForm.cshtml";
}
<script type="text/javascript">
    $(function () {
        InitControl();
    })
    //得到一个对象实体
    function InitControl() {
        if (!!GetQuery('KeyValue')) {
            AjaxJson("/CommonModule/QuaCheckItemBase/SetForm", { KeyValue: GetQuery('KeyValue') }, function (data) {
                SetWebControls(data);
            });
        }
    }
</script>
<form id="form1" style="margin: 1px">
    <table class="form">
        <tr>
            <th class='formTitle'>检查项主键：</th>
            <td class='formValue'>
                <label id="QuaCheckItemId" />
            </td>
            <th class='formTitle'>检查项编号：</th>
            <td class='formValue'>
                <label id="QuaCheckItemCd" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>检查项名称：</th>
            <td class='formValue'>
                <label id="QuaCheckItemNm" />
            </td>
            <th class='formTitle'>检查项类型：</th>
            <td class='formValue'>
                <label id="QuaCheckItemTy" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>检查项上限：</th>
            <td class='formValue'>
                <label id="QuaCheckItemUpLimit" />
            </td>
            <th class='formTitle'>检查项下限：</th>
            <td class='formValue'>
                <label id="QuaCheckItemLowLimit" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>检查项标准值：</th>
            <td class='formValue'>
                <label id="QuaCheckItemValue" />
            </td>
            <th class='formTitle'>检查项描述：</th>
            <td class='formValue'>
                <label id="QuaCheckItemDsc" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>有效性：</th>
            <td class='formValue'>
                <label id="Enabled" />
            </td>
            <th class='formTitle'>版本号：</th>
            <td class='formValue'>
                <label id="VersionNumber" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>创建时间：</th>
            <td class='formValue'>
                <label id="CreTm" />
            </td>
            <th class='formTitle'>创建人编号：</th>
            <td class='formValue'>
                <label id="CreCd" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>创建人名称：</th>
            <td class='formValue'>
                <label id="CreNm" />
            </td>
            <th class='formTitle'>修改时间：</th>
            <td class='formValue'>
                <label id="MdfTm" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>修改人编号：</th>
            <td class='formValue'>
                <label id="MdfCd" />
            </td>
            <th class='formTitle'>修改人名称：</th>
            <td class='formValue'>
                <label id="MdfNm" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>备注：</th>
            <td class='formValue'>
                <label id="Rem" />
            </td>
            <th class='formTitle'>预留字段1：</th>
            <td class='formValue'>
                <label id="RsvFld1" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>预留字段2：</th>
            <td class='formValue'>
                <label id="RsvFld2" />
            </td>
        </tr>
    </table>
</form>

@{
    ViewBag.Title = "质量录入信息界面>DPU更新表单界面";
    Layout = "~/Views/Shared/_LayoutForm.cshtml";
}
<script type="text/javascript">
    $(function () {
       
    })
    
   

    //保存事件
    function AcceptClick() {
        var date = $("#date").val();
        AjaxJson("/QualityModule/Q_CarQualityOutput_Pro/SubmitForm1?date=" + date, {}, function (data) {
            tipDialog(data.Message, 3, data.Code);
            top.frames[tabiframeId()].windowload();
            closeDialog();
        });
    }
</script>
<form id="form1" style="margin: 1px">
    <div id="message" style="display: none"></div>
    <table class="form">
        <tr>
            <th class='formTitle'>日期：</th>
            <td class='formValue' colspan="3">
                <input id="date" type="text" class="txt Wdate"  onfocus="WdatePicker({maxDate: '@DateTime.Now.ToString("yyyy-MM-dd")'})" value="@DateTime.Now.ToString("yyyy-MM-dd")" readonly />
            </td>
        </tr>
    </table>
</form>
